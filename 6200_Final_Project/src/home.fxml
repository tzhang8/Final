<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HomeController">
   <children>
      <Button id="btLogout" fx:id="btLogout" alignment="BOTTOM_LEFT" layoutX="700.0" layoutY="750.0" mnemonicParsing="false" onAction="#switchToLoginScene" text="logout" />
      <Button id="btTips" fx:id="btTips" layoutX="588.0" layoutY="297.0" mnemonicParsing="false" onAction="#switchToTipsScene" text="tips" />
      <Button id="btChart" fx:id="btChart" layoutX="163.0" layoutY="297.0" mnemonicParsing="false" onAction="#switchToChartScene" text="chart" />
      <Text layoutX="186.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Trip Geeks">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font>
      </Text>
      <Text layoutX="74.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You may check out our review database by clicking the chart button, all data has been visualized.  " wrappingWidth="270.0">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Text layoutX="472.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Travel tips for top four counties by reviews are provided here, you may take a look before making your plans." wrappingWidth="270.0">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <ImageView fx:id="picHome" fitHeight="315.0" fitWidth="490.0" layoutX="155.0" layoutY="400.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
